{% extends 'base.html.twig' %}

{% block title %}Valorant - STRANGE LEVEL SKINS
{% endblock %}

{% block body %}
<div class="container">
		<a class="btn btn-primary bg-dark mt-5" href="{{ path('app_valorant') }}">Return</a>
	</div>

	<div class="container mt-4 text-center">
		<div class="row justify-content-center mt-5">

			<h1 class="text-white">Strange model Skins</h1>
			
			{% for item in contenido| sort((a,b)=> a.rank <=> b.rank) %}
				<div class="col-md-2 ms-4 mt-5">
					<div class="card bg-dark text-white" style="width: 15rem;">
						<img src="{{ item.displayIcon }}" class="card-img-top mt-2 mx-auto d-block" style="max-width: 100px; max-height: 100px;">
						<div class="card-body">
							<p class="card-title text-white">{{ item.displayName }}
								-
								{{ item.devName }}</p>
							<p class="card-text text-white">Strange:
								{{ item.rank }}</p>
						</div>
					</div>
				</div>
			{% endfor %}
		</div>
	</div>
{% endblock %}
